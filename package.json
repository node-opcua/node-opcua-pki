{
  "name": "node-opcua-pki",
  "version": "1.5.4",
  "description": "PKI management for node-opcua",
  "main": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npx tsc",
    "test": "npx mocha -t 700000 test/**/*.ts",
    "cover": "npx istanbul cover -x \"bin/**\" node_modules/mocha/bin/_mocha",
    "cost-of-modules": "npx cost-of-modules --no-install",
    "release-it": "npx release-it",
    "pkg": "npx pkg -o pkg/pki bin/crypto_create_CA.js"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": "./crypo_create_CA_config.example.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/node-opcua/node-opcua-pki.git"
  },
  "bugs": {
    "url": "https://github.com/node-opcua/node-opcua-pki/issues"
  },
  "keywords": [
    "PKI",
    "node-opcua"
  ],
  "author": "Etienne Rossignon",
  "license": "MIT",
  "dependencies": {
    "async": "^3.1.0",
    "better-assert": "^1.0.2",
    "byline": "^5.0.0",
    "chalk": "^3.0.0",
    "cli-spinner": "^0.2.10",
    "cli-table": "^0.3.1",
    "del": "^5.1.0",
    "node-opcua-crypto": "^1.1.4",
    "progress": "^2.0.3",
    "source-map-support": "^0.5.16",
    "thenify": "^3.3.0",
    "underscore": "^1.9.1",
    "walk": "^2.3.14",
    "wget-improved": "^3.0.2",
    "yargs": "^14.2.0",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@types/async": "^3.0.3",
    "@types/byline": "^4.2.31",
    "@types/cli-spinner": "^0.2.0",
    "@types/cli-table": "^0.3.0",
    "@types/del": "^4.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.7",
    "@types/progress": "^2.0.3",
    "@types/underscore": "^1.9.3",
    "@types/yargs": "^13.0.3",
    "@types/yauzl": "^2.9.1",
    "mocha": "^6.2.2",
    "should": "^13.2.3",
    "ts-node": "^8.5.0",
    "typescript": "^3.7.2"
  },
  "bin": {
    "pki": "./bin/crypto_create_CA.js"
  }
}
